<UserControl x:Class="SSControlsNET.SSLEDTextPlayer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SSControlsNET"
             Loaded="UserControl_Loaded" Unloaded="UserControl_Unloaded" SizeChanged="UserControl_SizeChanged"
             TextOptions.TextFormattingMode="Ideal" TextOptions.TextRenderingMode="Auto">
    <Canvas Name="cvMain">
        <local:SSDrawingText x:Name="tplayer" HorzAlign="Center"/>
    </Canvas>
</UserControl>
